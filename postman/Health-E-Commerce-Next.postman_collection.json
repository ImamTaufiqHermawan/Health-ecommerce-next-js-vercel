    {
      "name": "External - Payment",
      "item": [
        {
          "name": "Create Payment",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    if (jsonData.data) {",
                  "        pm.environment.set('orderId', jsonData.data.orderId);",
                  "        pm.environment.set('paymentToken', jsonData.data.paymentToken);",
                  "    }",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "auth": {
              "type": "bearer",
              "bearer": [{ "key": "token", "value": "{{authToken}}" }]
            },
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"items\": [\n    {\n      \"id\": \"{{productId}}\",\n      \"name\": \"Vitamin C 1000mg\",\n      \"price\": 150000,\n      \"quantity\": 2\n    }\n  ],\n  \"customerDetails\": {\n    \"name\": \"John Doe\",\n    \"email\": \"john@example.com\",\n    \"phone\": \"08123456789\",\n    \"address\": \"Jl. Sudirman No. 123\"\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/external/payment/create",
              "host": ["{{baseUrl}}"],
              "path": ["api", "external", "payment", "create"]
            }
          }
        },
        {
          "name": "Payment Webhook (Midtrans)",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"order_id\": \"{{orderId}}\",\n  \"transaction_status\": \"settlement\",\n  \"status_code\": \"200\",\n  \"gross_amount\": \"300000\",\n  \"payment_type\": \"credit_card\",\n  \"transaction_id\": \"test-transaction-123\",\n  \"transaction_time\": \"2024-01-15 10:30:00\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/external/payment/webhook",
              "host": ["{{baseUrl}}"],
              "path": ["api", "external", "payment", "webhook"]
            }
          }
